{
    "restartable": "rs",
    "verbose": true,
    "execMap": {
        "": "babel-node",
        "js": "babel-node"
    },
    "events": {
        "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
    },
    "watch": [
        "bin",
        "views",
        "app.js"
    ],
    "env": {
        "NODE_ENV": "development",
        "PORT": "3003"
    },
    "ext": "js json"
}