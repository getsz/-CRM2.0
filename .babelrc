{
    "presets": [
        "stage-0",
        [
            "env",
            {
                "targets": {
                    "browser": [
                        "last 2 Chrome versions",
                        "ie >= 8",
                        "Firefox >= 20",
                        "iOS 7",
                        "> 5%"
                    ]
                }
            }
        ],
        "react"
    ],
    "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties" ,
        [
            "import",
            {
                "libraryName":"antd",
                "style":"css"
            }
        ]      
    ]
}